          "foregroundImage": "./assets/images/adaptive-icon.png",
          "backgroundColor": "#ffffff"
        }
      },
      "web": {
        "favicon": "./assets/images/favicon.png",
        "bundler": "metro"
      },
      "scheme": "myapp"
    },
    "extra": {
      "eas": {
        "projectId": "c4cc7bea-6c06-43bf-acc7-6726ba28a943"
      }
